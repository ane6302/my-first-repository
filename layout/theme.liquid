        </div>
        <div class="errors_div"></div>
        <div class="submit_btn btn button button--full-width button--primary"><span>{% if section.settings.notify_form_btn != blank %}{{ section.settings.notify_form_btn }}{% else %}Submit{% endif %}</span></div>
        <div class="popup-info">
          {% if section.settings.notify_form_information != blank %}
          <p>{{ section.settings.notify_form_information }}</p>
          {% endif %}
        </div>
      </div>
    </div>
    </form>
  </div>
</div>
<div class="notify-us-success">
  <div class="notify-us-content">
    <div class="close-success-popup close">{% render 'icon-close' %}</div>
    <h2>{{ section.settings.notify_form_sub_title }}</h2>
    <div class="content-box">
      {% if section.settings.notify_form_sub_information != blank %}
        <p>{{ section.settings.notify_form_sub_information }}</p>
      {% endif %}
      <button class="submit_btn close button button--full-width button--primary">{% if section.settings.notify_form_sub_btn != blank %}{{ section.settings.notify_form_sub_btn }}{% else %}Close{% endif %}</button>
    </div>
  </div>
</div>
<script>
$(document).ready(function(){
  var contact_form = $('body').find('form#notify-us-form');
   const validateEmail = (email) => {
    return email.match(
      /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
    );
  };
  
  $('body').on('click','.notify-us-icon',function(){
    $('body').addClass('overflow-hidden');
    contact_form.find('.errors_div').html('');
    $('body').find('.notify-us-popup').addClass('active');
  });
  
  $('body').on('click','.close-notify-popup',function(){
    $(this).closest('.notify-us-popup').removeClass('active');
    $('body').removeClass('overflow-hidden');
  });
  
  $('body').on('click','.notify-us-success .close',function(){
    $(this).closest('.notify-us-success').removeClass('active');
    $('body').removeClass('overflow-hidden');
  });
  
  $('body').on('click','.notify-us-popup .submit_btn',function(e){
    let name = contact_form.find('[name="contact[name]"]').val();
    if (name.trim() == ''){
      contact_form.find('.errors_div').html('<span class="error_text">{{ 'templates.contact.form.invalid_name' | t }}</span>');
      return;
    }
    
    let temp_email = contact_form.find('[name="contact[email]"]').val();
    if(!validateEmail(temp_email)){
      contact_form.find('.errors_div').html('<span class="error_text">{{ 'templates.contact.form.invalid_email' | t }}</span>');
      return;
    }

    let choices = contact_form.find('[name="contact[choices]"]:checked');
    if(choices.length == 0){
      contact_form.find('.errors_div').html('<span class="error_text">{{ 'templates.contact.form.choose_atleast_one' | t }}</span>');
      return;
    }
    
    contact_form.find('.errors_div').html('');
    //alert('submitted');
    submit_form(e);
  });
  
  $('#notify-us-form [type="email"]').on("keydown", function(e) { 
    if(e.which == 13){  
      e.preventDefault();
    }
  });
  
  var formData = {
      form: contact_form,
      formId: contact_form.attr('id'),
      action: contact_form.attr('action')
  };
  
  // Add submit event listener
  function submit_form(e) {
      e.preventDefault();
      console.log('submitting');


      // Init AJAX
      $.ajax({
          type: 'POST',
          async: true,
          url: formData.action,
          data: formData.form.serialize(),
          beforeSend: function() {
              formData.form.addClass('sending'); 
          },
          error: function(data) {
              console.log('error: ',data);
              //formData.form.addClass('error');
              
              // 429 error - too many requests
              if(data.status == 429) {
                  // Allow for default redirect to challenge form, do not force
                  console.log('Allow default window redirection for challenge');
                  formData.form.submit();
              }else {
                  console.log('current_status_error',data);
                  $(formData.form).find('.errors_div').html(data.statusText);
              }
              formData.form.removeClass('sending'); 
          },
          success: function (data) {
              console.log('success');
              formData.form.addClass('success');
              $('.notify-us-popup').removeClass('active');
              $('.notify-us-success').addClass('active');
              formData.form.removeClass('sending'); 
          }
      });// close ajax
  } // close 'submit' event

});

</script>
